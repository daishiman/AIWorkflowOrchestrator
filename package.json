{
  "name": "ai-workflow-orchestrator",
  "version": "1.0.0",
  "private": true,
  "description": "Universal AI Workflow Orchestrator - Monorepo",
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @repo/desktop dev",
    "dev:web": "pnpm --filter @repo/web dev",
    "build": "pnpm --filter @repo/desktop build",
    "build:web": "pnpm --filter @repo/web build",
    "build:all": "pnpm -r build",
    "test": "pnpm -r test:run",
    "test:watch": "vitest",
    "test:all": "pnpm --filter @repo/shared test:run && pnpm --filter @repo/desktop test:run",
    "test:coverage": "pnpm -r test:coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm -r exec rm -rf dist out node_modules/.vite",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^5.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.48.1",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.9.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "electron",
      "esbuild"
    ]
  }
}
