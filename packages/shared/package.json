{
  "name": "@repo/shared",
  "version": "1.0.0",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "import": "./dist/core/index.js"
    },
    "./infrastructure": {
      "types": "./dist/infrastructure/index.d.ts",
      "import": "./dist/infrastructure/index.js"
    },
    "./infrastructure/auth": {
      "types": "./dist/infrastructure/auth/index.d.ts",
      "import": "./dist/infrastructure/auth/index.js"
    },
    "./infrastructure/database": {
      "types": "./dist/infrastructure/database/index.d.ts",
      "import": "./dist/infrastructure/database/index.js"
    },
    "./types": {
      "types": "./dist/src/types/index.d.ts",
      "import": "./dist/src/types/index.js"
    },
    "./types/auth": {
      "types": "./dist/types/auth.d.ts",
      "import": "./dist/types/auth.js"
    },
    "./types/api-keys": {
      "types": "./dist/types/api-keys.d.ts",
      "import": "./dist/types/api-keys.js"
    },
    "./infrastructure/ai/apiKeyValidator": {
      "types": "./dist/infrastructure/ai/apiKeyValidator.d.ts",
      "import": "./dist/infrastructure/ai/apiKeyValidator.js"
    },
    "./schemas": {
      "types": "./dist/schemas/index.d.ts",
      "import": "./dist/schemas/index.js"
    },
    "./schemas/auth": {
      "types": "./dist/schemas/auth.d.ts",
      "import": "./dist/schemas/auth.js"
    },
    "./types/replace": {
      "types": "./dist/src/types/replace.d.ts",
      "import": "./dist/src/types/replace.js"
    },
    "./types/rag": {
      "types": "./dist/types/rag/index.d.ts",
      "import": "./dist/types/rag/index.js"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@supabase/supabase-js": "^2.86.2",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.39.0",
    "react-router-dom": "^7.11.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@vitest/coverage-v8": "^2.1.9",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
